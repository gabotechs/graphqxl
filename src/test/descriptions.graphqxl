"decription"
type Generic<T> {
    foo: T
}

"overriden"
type OverridenDescription = Generic<String>

type OriginalDescription = Generic<String>

"$GENERIC with inner type $T"
type Unknown<T> {
    "$GENERIC.foo has a type $T"
    foo: T
}

type Known = Unknown<OriginalDescription>

type Child {
    "My dad is $PARENT"
    child_foo: Int
}

type Parent {
    "My son is Child"
    parent_foo: Int
    ...Child
}